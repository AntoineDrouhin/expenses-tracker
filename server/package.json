    "cors": "2.8.1",
    "cross-env": "3.1.4",
    "dotenv": "4.0.0",
    "express": "4.14.0",
    "express-session": "1.15.1",
    "mongoose": "4.7.6",
    "passport": "0.3.2",
    "passport-local": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "eslint": "3.13.1",
    "mocha": "3.3.0",
    "nodemon": "1.11.0"
