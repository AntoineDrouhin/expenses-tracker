{
  "name": "expenses-tracker-server",
  "version": "1.0.0",
  "description": "An api to store expenses data for expenses-tracker app",
  "main": "source/server.js",
  "scripts": {
    "start": "cross-env NODE_PATH=source NODE_ENV=production node source/server.js",
    "start-dev": "cross-env NODE_PATH=source NODE_ENV=production nodemon source/server.js",
    "debug": "cross-env NODE_PATH=source NODE_ENV=production nodemon --inspect source/server.js",
